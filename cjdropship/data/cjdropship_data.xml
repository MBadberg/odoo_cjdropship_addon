<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        
        <!-- Default Configuration -->
        <record id="cjdropship_default_config" model="cjdropship.config">
            <field name="name">CJDropshipping Settings</field>
            <field name="active" eval="True"/>
            <field name="auto_sync_products" eval="False"/>
            <field name="sync_interval">24</field>
            <field name="auto_fulfill_orders" eval="False"/>
            <field name="default_product_type">consu</field>
            <field name="price_markup_type">percentage</field>
            <field name="price_markup">30.0</field>
            <field name="webhook_enabled" eval="True"/>
        </record>
        
    </data>
</odoo>
